section.section-white.case-study-deliverables-header
  .row.large-6.columns.centered.small-centered
    h5 Creators
    ul.creators
      each creator in case_study.creators
        -
          var employee = contentful.employees.filter(function(employee) {
            return creator.fields.name === employee.name;
          })[0]

        li
          a(href=employee._url)
            img(src=asset(employee.mugshot, { w: 60, h: 60 }), alt=employee.mugshot_alt_text)
